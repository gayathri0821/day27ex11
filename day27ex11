package march27;

 class access
 {
  int a;
  double b;
  public int x;	
  public int y;	
   public  void cal(int a, double b) 
   {
	
            x =  a + 1;	
            y = (int)b;
            } 
}    	
  public class Ex11 
  {	
     public static void main(String args[])
     {	
            access obj = new access();   	
            obj.cal(2, 3);	
            System.out.println(obj.x + ", " + obj.y);	
	
}	
    }		



